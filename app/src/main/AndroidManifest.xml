    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Material">
        <activity android:name=".MainActivity" />
        <activity android:name=".WordsClasses.WordViewActivity" />
        <activity android:name=".WordsClasses.AddWordActivity" />
        <activity android:name=".WordsClasses.ListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WordsClasses.EditWordActivity" />
        <activity android:name=".DeleteAllActivity" />
        <activity android:name=".CategoriesClasses.AddCategoryActivity" />
        <activity android:name=".CategoriesClasses.CategoryListActivity" />
        <activity android:name=".CategoriesClasses.CategoryViewActivity" />
        <activity android:name=".CategoriesClasses.EditCategoryActivity"></activity>
    </application>

</manifest>